{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["WebWorker", "ES2022", "DOM.Iterable"],
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "types": ["@cloudflare/workers-types"]
  },
  "include": ["src"]
}
